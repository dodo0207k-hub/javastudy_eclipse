package tutorial14;

public class cellect {
	public static void sum(int x, int y, int z) { //리턴 값이 없으며 매개변수가 존재하는 메소드
		System.out.println(x + y + z);
	}
	public static int force(int m, int a) { //리턴값이 있으며 매개변수가 존재하는 메소드
		return m * a;
	}
	public static void korean() { //리턴값이 없으며 매개변수가 존재하지 않는 메소드
		System.out.println("안녕하세요.");
	}
	
	public static void main(String[] args) {
		sum(10, 20, 30);
		System.out.println(force(20, 10));
		korean();
	}

}
